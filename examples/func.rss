(func mult ("" y z) (mul y z))
(call mult 5 5)

(func hello (1) (print 1))
(call hello)

// Factorial
(func fact ("" x) (if (eq? x 0) (base 1) (else (mul x (call fact (sub x 1))))))
(call fact 5)

// Summation
(func sum ("" x) (if (eq? x 0) (base 0) (else (add x (call sum (sub x 1))))))
(call sum 5)

// Fibonacci
(func fib ("" x) (if (or (eq? x 0) (eq? x 1)) (base x) (else (add (call fib (sub x 1)) (call fib (sub x 2))))))
(call fib 10)
(call fib 25)
